#include <stdio.h>

int main() {
    int m1, m2, m3, m4, m5, total;
    float percent;

    printf("Enter marks of 5 subjects: \n");

    printf("Subject 1: ");
    scanf("%d", &m1);

    printf("Subject 2: ");
    scanf("%d", &m2);

    printf("Subject 3: ");
    scanf("%d", &m3);

    printf("Subject 4: ");
    scanf("%d", &m4);

    printf("Subject 5: ");
    scanf("%d", &m5);

    total = m1 + m2 + m3 + m4 + m5;
    percent = total / 5.0;

    printf("\nTotal Marks = %d\n", total);
    printf("Percentage  = %.2f%%\n", percent);

    // Grade calculation (very simple)
    if (percent >= 90)
        printf("Grade = A\n");
    else if (percent >= 80)
        printf("Grade = B\n");
    else if (percent >= 70)
        printf("Grade = C\n");
    else if (percent >= 60)
        printf("Grade = D\n");
    else if (percent >= 50)
        printf("Grade = E\n");
    else
        printf("Grade = F\n");

    return 0
}
